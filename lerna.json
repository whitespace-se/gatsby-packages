{
  "command": {
    "publish": {
      "changelogPreset": {
        "name": "conventionalcommits",
        "issueUrlFormat": "{{host}}/{{owner}}/{{repository}}/issues/{{id}}"
      },
      "conventionalCommits": true,
      "ignoreChanges": [
        "CHANGELOG.md"
      ],
      "message": "chore(release): publish packages"
    }
  },
  "npmClient": "yarn",
  "packages": [
    "packages/*"
  ],
  "version": "1.0.0-beta.0"
}
